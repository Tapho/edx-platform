<% _.each(items, function (item) { %>
    <li class='search-results-item' role="region" aria-label="search result">
        <div class='sri-excerpt'><%- item.excerpt %> &hellip;</div>
        <span class='sri-type'><%- item.contentType %></span>
        <span class='sri-location'><%- stringifyLocation(item.location) %></span>
        <a class="sri-link" href="<%- item.url %>"><%= gettext("View") %> <i class="icon-arrow-right"></i></a>
    </li>
<% }); %>
